{
  "name": "multisafepay/php-sdk",
  "description": "MultiSafepay PHP SDK",
  "type": "library",
  "license": "MIT",
  "version": "2.0.0",
  "require": {
    "php": "^7.0",
    "ext-json": "*",
    "psr/http-message": "^1.0",
    "php-http/client-implementation": "^1.0",
    "php-http/httplug": "~1.1",
    "php-http/discovery": "^1.0",
    "php-http/message": "^1.7",
    "psr/http-factory": "^1.0",
    "ext-bcmath": "*",
    "league/iso3166": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^6.5",
    "php-http/guzzle6-adapter": "^2.0",
    "guzzlehttp/psr7": "^1.6",
    "object-calisthenics/phpcs-calisthenics-rules": "^3.5 || ^2.0",
    "http-interop/http-factory-guzzle": "^1.0",
    "php-http/mock-client": "^1.3",
    "fzaninotto/faker": "^1.9",
    "phpstan/phpstan": "^0.12.23"
  },
  "suggest": {
    "jschaedl/iban-validation": "Adds additional IBAN validation for \\MultiSafepay\\ValueObject\\IbanNumber"
  },
  "autoload": {
    "psr-4": {
      "MultiSafepay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MultiSafepay\\Tests\\": "tests/"
    }
  }
}
